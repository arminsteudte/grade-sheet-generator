                <mapping row="0" col="2">dates.date1</mapping>
                <mapping row="0" col="3">dates.date2</mapping>
                <mapping row="0" col="4">dates.date3</mapping>
		</section>
		<loop startRow="1" endRow="1" items="pupils" var="pupil" varType="de.as.garde_sheet_generator.model.Pupil">
